{
  "capabilities":
      [
        {
          "browserName": "firefox",
          "maxInstances": 10,
          "seleniumProtocol": "WebDriver"
        },
        {
          "browserName": "chrome",
          "maxInstances": 10,
          "seleniumProtocol": "WebDriver"
        }
      ],
  "configuration":
  {
    "proxy": "org.openqa.grid.selenium.proxy.DefaultRemoteProxy",
    "maxSession": <%= node['selenium']['node']['maxSession'] %>,
    "port": 5555,
    "host": "<%= node['ipaddress'] %>",
    "hostname": "<%= node['name'] %>",
    "register": true,
    "registerCycle": 5000,
    "hubPort": <%= node['selenium']['server']['hubport'] %>,
    "hubHost": <%= node['selenium']['node']['hubhost'] %>
  }
}
